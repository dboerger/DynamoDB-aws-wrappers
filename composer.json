{
    "name"        : "oasis/aws-wrappers",
    "description" : "Wrappers for AWS SDK",
    "type"        : "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dboerger/DynamoDB-php-event"
        }
    ],
    "require"     : {
        "aws/aws-sdk-php": "^3.22",
        "oasis/logging"  : "^1.3",
        "oasis/event"    : "dev-Timestampable",
        "doctrine/common": "^2.7"
    },
    "require-dev" : {
        "phpunit/phpunit"   : "^5.7",
        "league/uri"        : "^4.2"
    },
    "license"     : "MIT",
    "authors"     : [
        {
            "name" : "Minhao Zhang",
            "email": "minhao.zhang@qq.com"
        }
    ],
    "autoload"    : {
        "psr-4": {
            "Oasis\\Mlib\\AwsWrappers\\": "src/AwsWrappers/",
            "Oasis\\Mlib\\Logging\\"    : "src/Logging/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oasis\\Mlib\\AwsWrappers\\Test\\": "ut/"
        }
    }
}
